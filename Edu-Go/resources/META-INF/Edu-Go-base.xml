<idea-plugin>
  <extensions defaultExtensionNs="Educational">
    <configurator language="go" implementationClass="com.jetbrains.edu.go.GoConfigurator"/>
    <codeforcesLanguageProvider implementation="com.jetbrains.edu.go.codeforces.GoCodeforcesLanguageProvider"/>

    <!-- Hyperskill -->
    <configurator implementationClass="com.jetbrains.edu.go.stepik.hyperskill.GoHyperskillConfigurator" language="go" courseType="Hyperskill"/>
  </extensions>
  <extensions defaultExtensionNs="com.intellij">
    <experimentalFeature id="edu.course.hyperskill.go.support" percentOfUsers="0">
      <!--suppress PluginXmlCapitalization -->
      <description>Enables Go language support for JetBrains Academy</description>
    </experimentalFeature>
  </extensions>
</idea-plugin>