<idea-plugin package="com.jetbrains.edu.sql.kotlin">
  <!--suppress PluginXmlValidity -->
  <dependencies>
    <module name="sql-core"/>
    <module name="sql-jvm"/>
    <module name="jvm-core"/>
    <module name="Edu-Kotlin"/>

    <plugin id="org.jetbrains.kotlin"/>
    <plugin id="com.intellij.modules.java"/>
    <plugin id="com.intellij.database"/>
  </dependencies>

  <extensions defaultExtensionNs="Educational">
    <configurator language="SQL" environment="Kotlin" implementationClass="com.jetbrains.edu.sql.kotlin.SqlKtConfigurator"/>
  </extensions>
</idea-plugin>
