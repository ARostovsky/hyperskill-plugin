<idea-plugin package="com.jetbrains.edu.sql.java">
  <!--suppress PluginXmlValidity -->
  <dependencies>
    <module name="sql-core"/>
    <module name="sql-jvm"/>
    <module name="jvm-core"/>
    <module name="Edu-Java"/>

    <plugin id="com.intellij.modules.java"/>
    <plugin id="com.intellij.database"/>
  </dependencies>

  <extensions defaultExtensionNs="Educational">
    <configurator language="SQL" environment="Java" implementationClass="com.jetbrains.edu.sql.java.SqlJConfigurator"/>
  </extensions>
</idea-plugin>
