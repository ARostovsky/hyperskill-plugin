<idea-plugin xmlns:xi="http://www.w3.org/2001/XInclude">
    <id>com.jetbrains.edu.rust</id>
    <xi:include href="/META-INF/educational-core.xml" xpointer="xpointer(/idea-plugin/*)"/>
    <depends optional="true" config-file="Edu-Rust.xml">org.rust.lang</depends>

    <extensions defaultExtensionNs="Educational">
        <checkListener implementation="com.jetbrains.edu.learning.checker.CheckActionListener"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <projectService serviceInterface="org.rust.cargo.project.model.CargoProjectsService"
                        serviceImplementation="org.rust.cargo.project.model.impl.CargoProjectsServiceImpl"
                        overrides="true"/>
    </extensions>
</idea-plugin>
