<idea-plugin>
  <depends>com.intellij.modules.lang</depends>
  <extensions defaultExtensionNs="Educational">
    <configurator language="ObjectiveC" implementationClass="com.jetbrains.edu.cpp.CppConfigurator" displayName="Cpp"/>
    <configurator language="OCLight" implementationClass="com.jetbrains.edu.cpp.CppConfigurator" displayName="LightCpp"/>
  </extensions>

  <extensions defaultExtensionNs="com.intellij">
    <renameHandler implementation="com.jetbrains.edu.cpp.CppTaskRenameHandler" order="first"/>
    <renameHandler implementation="com.jetbrains.edu.cpp.CppLessonRenameHandler" order="first"/>
    <renameHandler implementation="com.jetbrains.edu.cpp.CppSectionRenameHandler" order="first"/>
  </extensions>
</idea-plugin>