<idea-plugin>
  <depends>com.intellij.modules.lang</depends>
  <depends>com.intellij.modules.python</depends>

  <extensions defaultExtensionNs="Educational">
    <pySdkSettingsHelper implementation="com.jetbrains.edu.python.learning.PyIdeaSdkSettingsHelper"/>
    <configurator implementationClass="com.jetbrains.edu.python.learning.PyIdeaConfigurator" language="Python"/>
    <configurator implementationClass="com.jetbrains.edu.python.learning.PyIdeaNewConfigurator" language="Python" environment="unittest" displayName="Python"/>
  </extensions>

</idea-plugin>
